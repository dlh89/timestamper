{
	"name": "timestamper",
	"displayName": "Timestamper",
	"description": "Insert a current Unix timestamp",
	"version": "2.0.0",
	"publisher": "dhide",
	"repository": {
		"type": "git",
		"url": "https://github.com/dlh89/timestamper"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.insertTimestamp",
		"onCommand:extension.insertTimestampMS"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.insertTimestamp",
				"title": "Insert timestamp"
			},
			{
				"command": "extension.insertTimestampMS",
				"title": "Insert timestamp (milliseconds)"
			}
		],
		"keybindings": [
			{
				"command": "extension.insertTimestamp",
				"key": "ctrl+alt+t",
				"mac": "ctrl+alt+t"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
